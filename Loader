--execute this for it to automatically load a script according to your game (null is cool)
local info = market:GetProductInfo(game.PlaceId, Enum.InfoType.Asset)
local games = {
    [{606849621}] = "https://raw.githubusercontent.com/Null7407/NullHub/main/Jailbreak",
    [{5910449407}] = "https://raw.githubusercontent.com/Null7407/NullHub/main/Project%20Menacing",
}

for ids, url in next, games do
    if table.find(ids, game.PlaceId) then
        loadstring(game:HttpGet(url))()
        break
        else
        print(info.Name)
    end
end
